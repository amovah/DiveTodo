{
  "name": "divetodo",
  "productName": "DiveTodo",
  "version": "1.5.1",
  "description": "Design your days and dive to them",
  "main": "build/index.js",
  "scripts": {
    "dev": "DEVELOPMENT=true ./node_modules/electron-prebuilt/cli.js ./build/",
    "prod": "./node_modules/electron-prebuilt/cli.js ./build/",
    "build-darwin": "./node_modules/electron-packager/cli.js . --platform=darwin --arch=x64 --prune",
    "build-linux": "./node_modules/electron-packager/cli.js . --platform=linux --arch=x64 --prune",
    "build-windows": "echo \"I hate windows, I love Linux. Use Linux instead.\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amovah/divetodo.git"
  },
  "author": "Ali Movahedi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amovah/divetodo/issues"
  },
  "homepage": "https://github.com/amovah/divetodo#readme",
  "directories": {
    "lib": "src"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "dependencies": {
    "chart.js": "^2.1.2",
    "electron-prebuilt": "^1.1.0",
    "moment": "^2.12.0",
    "react": "^0.14.7",
    "react-day-picker": "^1.1.5",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "redux": "^3.3.1",
    "stringing": "^1.6.6"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "electron-packager": "^7.0.2",
    "eslint-plugin-react": "^4.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^2.0.0",
    "gulp-less": "^3.0.5",
    "gulp-rimraf": "^0.2.0"
  }
